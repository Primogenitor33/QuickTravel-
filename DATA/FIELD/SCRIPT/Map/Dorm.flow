void QTMapDormTravel()
{
	int page = 1;
	while(true)
	{
		switch(page)
		{	
			case 0:
				return;
			case 1:
				page = DormTravel1();
				break;
			case 2:
				page = DormTravel2();
				break;
			 default:
				break;
		}
	}
}

int DormTravel1()
{
	bool male = ( BIT_CHK( 3081 ) == 1 ) || ( BIT_CHK( 3082 ) == 1 );
	bool female = ( BIT_CHK( 3084 ) == 1 ) || ( BIT_CHK( 3083 ) == 1 ); 
	OPEN_MSG_WIN();
	MSG( Choose );
	FUNCTION_00B7(0);
	FUNCTION_011C(12, Page1of2);
	//Close menu on Circle
	SEL_CHK_PAD(6,20);
	//Next Page on D-Pad Right
	SEL_CHK_PAD(13,4);
	//Previous Page on D-Pad Left
	SEL_CHK_PAD(15,5);
	int select = SEL( Dorm );
	FUNCTION_0005();
	CLOSE_MSG_WIN();
	switch (select)
	{
		case 0: // Dorm Entrance
			DormEntrance();
			break;
		case 1: // Your Room
			DormRoom();
			break;
		case 2: // 1st Floor, Lounge
			DormLobby();
			break;
		case 3: // 2nd Floor, Hallway
			DormFloorTwo();
			break;
		case 4: // Next Page (D-Pad Right)
			return 2;
			break;
		case 5: // Previous Page (D-Pad Left)
			return 2;
			break;
		default:
			DormMap();
			break;
	}
	return 0;
}

int DormTravel2()
{
	OPEN_MSG_WIN();
	MSG( Choose );
	FUNCTION_00B7(0);
	FUNCTION_011C(12, Page2of2);
	//Close menu on Circle
	SEL_CHK_PAD(6,20);
	//Next Page on D-Pad Right
	SEL_CHK_PAD(13,4);
	//Previous Page on D-Pad Left
	SEL_CHK_PAD(15,5);
	int select = SEL( Dorm2 );
	FUNCTION_0005();
	CLOSE_MSG_WIN();
	switch (select)
	{
		case 0: // 3rd Floor, Hallway
			DormFloorThree();
			break;
		case 1: // 4th Floor, Hallway
			DormFloorFour();
			break;
		case 2: // Meeting Room
			MeetingRoom();
			break;
		case 4: // Next Page (D-Pad Right)
			return 1;
			break;
		case 5: // Previous Page (D-Pad Left)
			return 1;
			break;
		default:
			DormMap();
			break;
	}
	return 0;
}

void DormEntrance()
{
	BIT_OFF( 3090 );
	BIT_OFF( 3091 );
	BIT_OFF( 3092 );
	BIT_OFF( 3093 );
	FUNCTION_00BC( 5 );
	FUNCTION_00BF();
	FUNCTION_000C( 10 );
	FADE( 1, 0 );
	FADE_SYNC();
	CALL_FIELD( 7, 9, 2, 0 );
	FUNCTION_00BE();
	return;
}

