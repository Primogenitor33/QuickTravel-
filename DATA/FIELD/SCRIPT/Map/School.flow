void SchoolNight()
{
	BIT_OFF( 3090 );
	BIT_OFF( 3091 );
	BIT_OFF( 3092 );
	BIT_OFF( 3093 );
	FUNCTION_00BC( 0 );
	FUNCTION_00BF();
	FUNCTION_000C( 10 );
	FADE( 1, 0 );
	FADE_SYNC();
	CALL_FIELD( 6, 2, 1, 0 );
	FUNCTION_00BE();
	return;
}

void QTMapSchoolTravel()
{
	int page = 1;
	while(true)
	{
		switch(page)
		{	
			case 0:
				return;
			case 1:
				page = SchoolTravel1();
				break;
			case 2:
				page = SchoolTravel2();
				break;
			case 3:
				page = SchoolTravel3();
				break;
			 default:
				break;
		}
	}
}

int SchoolTravel1()
{
	OPEN_MSG_WIN();
	MSG( Choose );
	FUNCTION_00B7(0);
	FUNCTION_011C(12, Page1of3);
	//Close menu on Circle
	SEL_CHK_PAD(6,20);
	//Next Page on D-Pad Right
	SEL_CHK_PAD(13,4);
	//Previous Page on D-Pad Left
	SEL_CHK_PAD(15,5);
	int select = SEL( School );
	FUNCTION_0005();
	CLOSE_MSG_WIN();
	switch (select)
	{
		case 0: // School Entranceway
			SchoolEntrance();
			break;
		case 1: // 1st Floor, Main Lobby
			SchoolLobby();
			break;
		case 2: // 1st Floor, Faculty Hall
			SchoolFacultyHall();
			break;
		case 3: //1st Floor, Laboratory Hall
			SchoolLaboratoryHall();
			break;
		case 4: // Next Page (D-Pad Right)
			return 2;
		case 5: //Previous Page (D-Pad Left)
			return 3;
		default:
			SchoolMap();
			break;
	}
	return 0;
}

