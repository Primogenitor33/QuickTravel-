void QuickTravel_Dorm()
{
	bool male = ( BIT_CHK( 3081 ) == 1 ) || ( BIT_CHK( 3082 ) == 1 );
	bool female = ( BIT_CHK( 3084 ) == 1 ) || ( BIT_CHK( 3083 ) == 1 ); 
	bool operation = ( FUNCTION_0083() == 8 ) && ( ( ( GET_MONTH() == 7 ) && ( GET_DAY_OF_MONTH() == 7 ) ) || ( ( GET_MONTH() == 11 ) && ( GET_DAY_OF_MONTH() == 3 ) ) || ( ( GET_MONTH() == 11 ) && ( GET_DAY_OF_MONTH() == 22 ) ) );
	if( BIT_CHK( 3584 ) == 1 )
	{
		Chk_Access_Dorm_003_015_Junpei();
		return;
	}
	if(operation)
	{
		Chk_Access_Dorm_006_005();
		return;
	}
	switch(GET_FIELD_MINOR())
	{
		case 1: // Your Room
			Open_Dorm_Win_001();
			break;
		case 2: // 1F, Lounge
			Open_Dorm_Win_002();
			break;
		case 3: // 2F, Hallway
			Open_Dorm_Win_003();
			break;
		case 4: // 3F, Hallway
			Open_Dorm_Win_004();
			break;
		case 5: // 4F, Hallway
			Open_Dorm_Win_005();
			break;
		case 6: // Meeting Room
			Open_Dorm_Win_006();
			break;
		case 7: // 5F, Hallway
			Open_Dorm_Win_007();
			break;
		case 9: // Dorm Entrance
			Open_Dorm_Win_009();
			break;
		case 14: // Vacant Room (Shinjiro)
			Open_Dorm_Win_014();
		case 15: // Vacant Room (Junpei, Ken, Fuuka, & Aigis)
			if(male)
			{
				Open_Dorm_Win_015_Male();
			}
			else if(female)
			{
				Open_Dorm_Win_015_Female();
			}
			break;
		default:
			No_QuickTravel();
			break;
		}
}

void MallTravel()
{
	bool operation = ( GET_MONTH() == 9 ) && ( GET_DAY_OF_MONTH() == 5 ) && ( FUNCTION_0083() == 8 );
	if(operation)
	{
		Chk_Access_PaulowniaMall_001_002();
		return;
	}
	OPEN_MSG_WIN();
	MSG( Choose );
	FUNCTION_00B7(0);
	FUNCTION_011C(12, Page1of1);
	//Close menu on Circle
	SEL_CHK_PAD(6,20);
	//Open map on Select
	SEL_CHK_PAD(8,6);
	//Open shops on Start
	SEL_CHK_PAD(11,7);
	int select = SEL( Mall );
	CLOSE_MSG_WIN();
	if (select < 2)
	{
		FADE( 3, 0 );
		FADE_SYNC();
	}
	switch (select)
	{
		case 0: // Paulownia Mall
			MallLobby();
			break;
		case 1: // Back Alley
			MallAlley();
			break;	
		case 2: // Facilities
			MallFacilities();
			break;
		case 3: // Town Map
			MallMap();
			break;
		case 6: //Town Map (Select)
			MallMap();
			break;
		case 7: //Shops (Start)
			MallFacilities();
			break;
		default:
			break;
	}
}

